{
  "name": "blankpage",
  "version": "1.7.0",
  "description": "Composing static html blog pages",
  "main": "dist/index.js",
  "bin": {
    "blankpage": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bridzius/blankpage.git"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "preversion": "NODE_ENV=test mocha -r ts-node/register src/*.test.ts && tslint src/**/*.ts",
    "compile": "tsc",
    "test": "jest",
    "start": "tsc -w",
    "verify": "eslint 'src/**/*.ts'"
  },
  "files": [
    "dist/",
    "!dist/**/*.test.js"
  ],
  "author": "Jurgis Bridzius",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/marked": "^0.7.2",
    "@types/node": "^13.1.6",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "marked": "^0.8.0"
  }
}
